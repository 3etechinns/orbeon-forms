<xbl:xbl xmlns:xhtml="http://www.w3.org/1999/xhtml"
         xmlns:xforms="http://www.w3.org/2002/xforms"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:ev="http://www.w3.org/2001/xml-events"
         xmlns:xi="http://www.w3.org/2001/XInclude"
         xmlns:xxi="http://orbeon.org/oxf/xml/xinclude"
         xmlns:xxforms="http://orbeon.org/oxf/xml/xforms"
         xmlns:fr="http://orbeon.org/oxf/xml/form-runner"
         xmlns:saxon="http://saxon.sf.net/"
         xmlns:xbl="http://www.w3.org/ns/xbl">

    <xbl:binding id="fr-date-picker" element="fr|date-picker">
        <metadata xmlns="http://orbeon.org/oxf/xml/form-builder">
            <display-name lang="en">Date picker</display-name>
            <display-name lang="fr">Selection de date</display-name>
            <icon lang="en">
                <small-icon>/apps/fr/style/images/silk/date.png</small-icon>
                <large-icon>/apps/fr/style/images/silk/date.png</large-icon>
            </icon>
            <datatype>xforms:date</datatype>
            <template>
                <fr:date-picker>
                    <xforms:label ref=""/>
                    <xforms:hint ref=""/>
                    <xforms:help ref=""/>
                    <xforms:alert ref=""/>
               </fr:date-picker>
            </template>
        </metadata>
        <xbl:template>
            <xforms:group xbl:attr="ref bind"><xbl:content includes="xforms|label,xforms|help,xforms|hint"/><xforms:output ref="."/>
                <xforms:input ref="." appearance="minimal"/>
            </xforms:group>
        </xbl:template>
    </xbl:binding>

</xbl:xbl>