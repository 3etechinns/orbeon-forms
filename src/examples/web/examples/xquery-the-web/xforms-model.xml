<xforms:model xmlns:xforms="http://www.w3.org/2002/xforms">
    <xforms:instance>
        <instance>
            <url>http://www.orbeon.com/community/downloads</url>
            <xquery-type>inline</xquery-type>
            <xquery-url/>
<xquery>let 
    $v := //table[tr[1]/th[1] = 'Version']/tr[2]/td[1] 
return
    &lt;p>Latest versions of Orbeon products:
        &lt;ul>
            &lt;li>PresentationServer: {string($v[1])}&lt;/li>
            &lt;li>Studio: {string($v[2])}&lt;/li>
        &lt;/ul>
    &lt;/p></xquery>
            <output>xml</output>
            <action/>
        </instance>
    </xforms:instance>
</xforms:model>
