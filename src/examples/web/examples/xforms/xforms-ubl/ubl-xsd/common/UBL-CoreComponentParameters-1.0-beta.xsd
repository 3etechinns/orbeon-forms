<xsd:schema version="1:0-beta"
 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 targetNamespace="urn:oasis:names:tc:ubl:CoreComponentParameters:1:0-beta"
 xmlns="urn:oasis:names:tc:ubl:CoreComponentParameters:1:0-beta"
 xmlns:ccts="urn:oasis:names:tc:ubl:CoreComponentParameters:1:0-beta"
 elementFormDefault="qualified"
 attributeFormDefault="unqualified">
  <xsd:element name="Component" type="ccts:ComponentType"/>
  <xsd:complexType name="ComponentType">
    <xsd:sequence>
      <xsd:element ref="CategoryCode" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="DictionaryEntryName" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="Definition" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="QualifierObjectClass" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="ObjectClass" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="QualifierPropertyTerm" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="PropertyTerm" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="QualifierRepresentationTerm" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="RepresentationTerm" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="QualifierDataType" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="DataType" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="QualifierAssociatedObjectClass" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="AssociatedObjectClass" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="BusinessTerm" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:element name="Context" type="ccts:ContextType"/>
  <xsd:complexType name="ContextType">
  	<xsd:sequence>
   		<xsd:element ref="IndustryClassification" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="Geopolitical" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="BusinessProcess" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="OfficialConstraint" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="ProductClassification" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="BusinessProcessRole" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="SupportingRole" minOccurs="0" maxOccurs="unbounded"/>
   		<xsd:element ref="SystemCapability" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>

  <xsd:element name="Contextualization" type="ccts:ContextualizationType"/>
  <xsd:complexType name="ContextualizationType">
  	<xsd:sequence>
	  	<xsd:element ref="Context" minOccurs="0" maxOccurs="unbounded"/>
  	</xsd:sequence>
  </xsd:complexType>

  <xsd:element name="Instance" type="ccts:InstanceType"/>
  <xsd:complexType name="InstanceType">
    <xsd:sequence>
      <xsd:element ref="Prefix" minOccurs="0" maxOccurs="1"/>
      <!--
           The following elements are to be used in Code List Schemas
           to record the metadata of the code list for which the schema
           represents.
           Examples are:
              CountryCode:
                  CodeListQualifier="ISO 3166"
                  CodeListAgency="6"
                  CodeListVersion="0.3"
              CurrencyCode:
                  CodeListQualifier="ISO 4217"
                  CodeListAgency="6"
                  CodeListVersion="0.3"
      -->
      <xsd:element ref="CodeListQualifier" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="CodeListAgency" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="CodeListVersion" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:element name="CategoryCode" type="xsd:token"/>
  <xsd:element name="DictionaryEntryName" type="xsd:string"/>
  <xsd:element name="Definition" type="xsd:string"/>
  <xsd:element name="QualifierObjectClass" type="xsd:token"/>
  <xsd:element name="ObjectClass" type="xsd:token"/>
  <xsd:element name="QualifierPropertyTerm" type="xsd:token"/>
  <xsd:element name="PropertyTerm" type="xsd:token"/>
  <xsd:element name="QualifierRepresentationTerm" type="xsd:token"/>
  <xsd:element name="RepresentationTerm" type="xsd:token"/>
  <xsd:element name="QualifierDataType" type="xsd:token"/>
  <xsd:element name="DataType" type="xsd:token"/>
  <xsd:element name="QualifierAssociatedObjectClass" type="xsd:token"/>
  <xsd:element name="AssociatedObjectClass" type="xsd:token"/>
  <xsd:element name="BusinessTerm" type="xsd:string"/>
  <xsd:element name="IndustryClassification" type="xsd:string"/>
  <xsd:element name="Geopolitical" type="xsd:string"/>
  <xsd:element name="BusinessProcess" type="xsd:string"/>
  <xsd:element name="OfficialConstraint" type="xsd:string"/>
  <xsd:element name="ProductClassification" type="xsd:string"/>
  <xsd:element name="BusinessProcessRole" type="xsd:string"/>
  <xsd:element name="SupportingRole" type="xsd:string"/>
  <xsd:element name="SystemCapability" type="xsd:string"/>
  <xsd:element name="Prefix" type="xsd:token"/>
  <xsd:element name="CodeListQualifier" type="xsd:token"/>
  <xsd:element name="CodeListAgency" type="xsd:token"/>
  <xsd:element name="CodeListVersion" type="xsd:token"/>
</xsd:schema>
