<xf:model xmlns:xf="http://www.w3.org/2002/xforms" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xf:instance>
		<form>
			<action/>
			<example-id>pdf-upload</example-id>
			<!-- Using xs:anyURI will cause the XForms engine to store a reference to
                 a URI instead of inlinig the content of the file -->
			<file filename="" mediatype="" size="" xsi:type="xs:anyURI"/>
		</form>
	</xf:instance>
	<xf:submission method="post" encoding="multipart/form-data"/>
</xf:model>
